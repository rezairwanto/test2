<!DOCTYPE html>
<html>
<head>
  <title>Final project</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tags %>
</head>
<body>
<h1>Welcome to my store</h1>
<span>----------------------------</span>
<div id="user_nav">  
<%= raw(welcome_text) %>  
</div>
<% flash.each do |name, msg| %> 
<%= content_tag :div, msg, :id => "flash#{name}" %> 
<% end %>
<%= yield %>

</body>
</html>
